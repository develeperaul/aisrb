.range-filter
  .range-filter__label test
  .range-filter__input
    .price__wrapper
      span.price-from
        span.prefix  от
        span.number
        span.prefix  м²
      span.prefix.separator  —
      span.price-to
        span.prefix  до
        span.number
        span.prefix  м²
    .multi-range-slider
      input.input-left(type='range' min='1000' step='1' max='10000' value='1000')
      input.input-right(type='range' min='0' step='1' max='10000' value='10000')
      .slider
        .track
        .range